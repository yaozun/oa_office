    <startEvent id="startevent1" name="Start" activiti:initiator="applyUserId"></startEvent>
    <userTask id="deptLeaderAudit" name="部门经理审批" activiti:candidateUsers="${leaveService.findDeptLeaders(applyUserId)}"></userTask>
